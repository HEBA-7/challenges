    "def solution(number):\n",
    "    sum = 0\n",
    "    if number < 0:\n",
    "        return 0\n",
    "    for i in range(number):\n",
    "        if (i % 3 ==0) or (i % 5 == 0):\n",
    "            sum += i\n",
    "    return sum"
   ]
